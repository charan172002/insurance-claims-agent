{
  "info": {
    "name": "Insurance Claims Processing Agent",
    "description": "API collection for testing the Insurance Claims Processing Agent",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Health Check",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "http://localhost:8080/api/v1/claims/health",
          "protocol": "http",
          "host": ["localhost"],
          "port": "8080",
          "path": ["api", "v1", "claims", "health"]
        },
        "description": "Check if the service is running"
      }
    },
    {
      "name": "Process Claim - Upload PDF",
      "request": {
        "method": "POST",
        "header": [],
        "body": {
          "mode": "formdata",
          "formdata": [
            {
              "key": "file",
              "type": "file",
              "src": "/path/to/your/claim.pdf",
              "description": "FNOL PDF document"
            }
          ]
        },
        "url": {
          "raw": "http://localhost:8080/api/v1/claims/process",
          "protocol": "http",
          "host": ["localhost"],
          "port": "8080",
          "path": ["api", "v1", "claims", "process"]
        },
        "description": "Upload and process an FNOL claim document"
      }
    },
    {
      "name": "Swagger UI",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "http://localhost:8080/swagger-ui.html",
          "protocol": "http",
          "host": ["localhost"],
          "port": "8080",
          "path": ["swagger-ui.html"]
        },
        "description": "Access the Swagger UI documentation"
      }
    }
  ]
}
